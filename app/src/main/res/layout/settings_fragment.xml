<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/mild4">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <RelativeLayout
            android:id="@+id/rlaLblSettings"
            android:layout_width="match_parent"
            android:layout_height="150dp"
            android:background="@color/primary_light"
            android:paddingBottom="@dimen/thirty">

            <ImageView
                android:id="@+id/imgBack"
                android:layout_width="@dimen/thirty"
                android:layout_height="@dimen/thirty"
                android:layout_marginStart="@dimen/thirty"
                android:layout_marginTop="@dimen/thirty"
                android:src="@drawable/arrow_back_black"
                android:visibility="gone"></ImageView>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/imgBack"
                android:layout_marginStart="@dimen/thirty"
                android:layout_marginTop="@dimen/fifty"
                android:text="@string/permission_settings"
                android:textColor="@color/black"
                android:textSize="@dimen/twenty_sp"
                android:fontFamily="@font/poppins_bold"
                android:textStyle="bold">

            </TextView>
        </RelativeLayout>

        <TextView
            android:id="@+id/lblGeneral"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/rlaLblSettings"
            android:layout_marginStart="@dimen/twenty"
            android:layout_marginTop="@dimen/twenty"
            android:text="@string/lblGENERAL"
            android:textStyle="normal"
            android:fontFamily="@font/poppins_regular"
            android:textSize="@dimen/fifteen_sp">

        </TextView>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/lblGeneral"
            android:layout_marginTop="@dimen/ten">

            <RelativeLayout
                android:id="@+id/rytGeneral"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="@dimen/fifteen"
                android:background="@drawable/rect_white">

                <LinearLayout
                    android:id="@+id/lnrNotification"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/thirty"
                    android:layout_marginTop="@dimen/twenty"
                    android:layout_marginEnd="@dimen/thirty"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:weightSum="1">

                    <ImageView
                        android:layout_width="@dimen/thirty"
                        android:layout_height="@dimen/thirty"
                        android:layout_weight="0.1"
                        android:src="@drawable/notification_icon"></ImageView>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginStart="@dimen/thirty"
                        android:layout_weight="0.8"
                        android:text="@string/lblNotifications"
                        android:textColor="@color/black"
                        android:textSize="@dimen/thirteen_sp"
                        android:fontFamily="@font/poppins_bold"
                        android:textStyle="bold">

                    </TextView>

                    <ImageView
                        android:layout_width="@dimen/twenty_five_sp"
                        android:layout_height="@dimen/twenty_five_sp"
                        android:layout_weight="0.1"
                        android:src="@drawable/arrow_right_gray">

                    </ImageView>
                </LinearLayout>

                <View
                    android:id="@+id/isViewNotification"
                    android:layout_width="match_parent"
                    android:layout_height="0.1dp"
                    android:layout_below="@+id/lnrNotification"
                    android:layout_marginStart="@dimen/thirty"
                    android:layout_marginTop="@dimen/ten"
                    android:layout_marginEnd="@dimen/thirty"
                    android:background="@color/grey">

                </View>

                <LinearLayout
                    android:id="@+id/lnrFaq"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/isViewNotification"
                    android:layout_marginStart="@dimen/thirty"
                    android:layout_marginTop="@dimen/twenty"
                    android:layout_marginEnd="@dimen/thirty"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:weightSum="1">

                    <ImageView
                        android:layout_width="@dimen/thirty"
                        android:layout_height="@dimen/thirty"
                        android:layout_weight="0.1"
                        android:src="@drawable/faq_icon"></ImageView>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginStart="@dimen/thirty"
                        android:layout_weight="0.8"
                        android:text="@string/lblFAQ"
                        android:textColor="@color/black"
                        android:textSize="@dimen/thirteen_sp"
                        android:fontFamily="@font/poppins_bold"
                        android:textStyle="bold">

                    </TextView>

                    <ImageView
                        android:layout_width="@dimen/twenty_five_sp"
                        android:layout_height="@dimen/twenty_five_sp"
                        android:layout_weight="0.1"
                        android:src="@drawable/arrow_right_gray">

                    </ImageView>
                </LinearLayout>

                <View
                    android:id="@+id/isViewFaq"
                    android:layout_width="match_parent"
                    android:layout_height="0.1dp"
                    android:layout_below="@+id/lnrFaq"
                    android:layout_marginStart="@dimen/thirty"
                    android:layout_marginTop="@dimen/ten"
                    android:layout_marginEnd="@dimen/thirty"
                    android:background="@color/grey">

                </View>

                <LinearLayout
                    android:id="@+id/lnrContactUs"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/isViewFaq"
                    android:layout_marginStart="@dimen/thirty"
                    android:layout_marginTop="@dimen/twenty"
                    android:layout_marginEnd="@dimen/thirty"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:weightSum="1">

                    <ImageView
                        android:layout_width="@dimen/thirty"
                        android:layout_height="@dimen/thirty"
                        android:layout_weight="0.1"
                        android:src="@drawable/contact_us_icon"></ImageView>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginStart="@dimen/thirty"
                        android:layout_weight="0.8"
                        android:text="@string/lblContact"
                        android:textColor="@color/black"
                        android:textSize="@dimen/thirteen_sp"
                        android:fontFamily="@font/poppins_bold"
                        android:textStyle="bold">

                    </TextView>

                    <ImageView
                        android:layout_width="@dimen/twenty_five_sp"
                        android:layout_height="@dimen/twenty_five_sp"
                        android:layout_weight="0.1"
                        android:src="@drawable/arrow_right_gray">

                    </ImageView>
                </LinearLayout>

                <View
                    android:id="@+id/isViewContactUs"
                    android:layout_width="match_parent"
                    android:layout_height="0.1dp"
                    android:layout_below="@+id/lnrContactUs"
                    android:layout_marginStart="@dimen/thirty"
                    android:layout_marginTop="@dimen/ten"
                    android:layout_marginEnd="@dimen/thirty"
                    android:background="@color/grey">

                </View>

                <LinearLayout
                    android:id="@+id/lnrTermsConditions"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/isViewContactUs"
                    android:layout_marginStart="@dimen/thirty"
                    android:layout_marginTop="@dimen/twenty"
                    android:layout_marginEnd="@dimen/thirty"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:weightSum="1">

                    <ImageView
                        android:layout_width="@dimen/twenty_five"
                        android:layout_height="@dimen/twenty_five"
                        android:layout_weight="0.1"
                        android:src="@drawable/terms_conditions_icon"></ImageView>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginStart="@dimen/thirty"
                        android:layout_weight="0.8"
                        android:text="@string/lblTermsCondition"
                        android:textColor="@color/black"
                        android:textSize="@dimen/thirteen_sp"
                        android:fontFamily="@font/poppins_bold"
                        android:textStyle="bold">

                    </TextView>

                    <ImageView
                        android:layout_width="@dimen/twenty_five_sp"
                        android:layout_height="@dimen/twenty_five_sp"
                        android:layout_weight="0.1"
                        android:src="@drawable/arrow_right_gray">

                    </ImageView>
                </LinearLayout>



                <View
                    android:id="@+id/isViewTermsConditions"
                    android:layout_width="match_parent"
                    android:layout_height="0.1dp"
                    android:layout_below="@+id/lnrTermsConditions"
                    android:layout_marginStart="@dimen/thirty"
                    android:layout_marginTop="@dimen/ten"
                    android:layout_marginEnd="@dimen/thirty"
                    android:background="@color/grey">

                </View>


                <LinearLayout
                    android:id="@+id/lnrLanguage"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/isViewTermsConditions"
                    android:layout_marginStart="@dimen/thirty"
                    android:layout_marginTop="@dimen/twenty"
                    android:layout_marginEnd="@dimen/thirty"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:weightSum="1">

                    <ImageView
                        android:layout_width="@dimen/twenty_five"
                        android:layout_height="@dimen/twenty_five"
                        android:layout_weight="0.1"
                        android:src="@drawable/ic_language"></ImageView>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginStart="@dimen/thirty"
                        android:layout_weight="0.8"
                        android:text="@string/lblChangeLanguage"
                        android:textColor="@color/black"
                        android:textSize="@dimen/thirteen_sp"
                        android:fontFamily="@font/poppins_bold"
                        android:textStyle="bold">

                    </TextView>

                    <ImageView
                        android:layout_width="@dimen/twenty_five_sp"
                        android:layout_height="@dimen/twenty_five_sp"
                        android:layout_weight="0.1"
                        android:src="@drawable/arrow_right_gray">

                    </ImageView>


                </LinearLayout>

                <View
                    android:id="@+id/isViewAppLanguage"
                    android:layout_width="match_parent"
                    android:layout_height="0.1dp"
                    android:layout_below="@+id/lnrLanguage"
                    android:layout_marginStart="@dimen/thirty"
                    android:layout_marginTop="@dimen/ten"
                    android:layout_marginEnd="@dimen/thirty"
                    android:background="@color/grey"/>


                <LinearLayout
                    android:id="@+id/lnrChangePassword"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/isViewAppLanguage"
                    android:layout_marginStart="@dimen/thirty"
                    android:layout_marginTop="@dimen/twenty"
                    android:layout_marginBottom="@dimen/ten"
                    android:layout_marginEnd="@dimen/thirty"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:weightSum="1">

                    <ImageView
                        android:layout_width="@dimen/twenty_five"
                        android:layout_height="@dimen/twenty_five"
                        android:layout_weight="0.1"
                        android:src="@drawable/ic_change_password">

                    </ImageView>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginStart="@dimen/thirty"
                        android:layout_weight="0.8"
                        android:text="@string/change_password"
                        android:textColor="@color/black"
                        android:textSize="@dimen/thirteen_sp"
                        android:fontFamily="@font/poppins_bold"
                        android:textStyle="bold">

                    </TextView>

                    <ImageView
                        android:layout_width="@dimen/twenty_five_sp"
                        android:layout_height="@dimen/twenty_five_sp"
                        android:layout_weight="0.1"
                        android:src="@drawable/arrow_right_gray">

                    </ImageView>
                </LinearLayout>


                <View
                    android:id="@+id/isViewSaveContact"
                    android:layout_width="match_parent"
                    android:layout_height="0.1dp"
                    android:layout_below="@+id/lnrChangePassword"
                    android:layout_marginStart="@dimen/thirty"
                    android:layout_marginTop="@dimen/ten"
                    android:layout_marginEnd="@dimen/thirty"
                    android:background="@color/grey"/>


                <LinearLayout
                    android:id="@+id/lnrSaveContact"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/isViewSaveContact"
                    android:layout_marginStart="@dimen/thirty"
                    android:layout_marginTop="@dimen/twenty"
                    android:layout_marginBottom="@dimen/fifteen"
                    android:layout_marginEnd="@dimen/thirty"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:weightSum="1">

                    <ImageView
                        android:layout_width="@dimen/twenty_five"
                        android:layout_height="@dimen/twenty_five"
                        android:layout_weight="0.1"
                        android:src="@drawable/ic_contact">

                    </ImageView>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginStart="@dimen/thirty"
                        android:layout_weight="0.8"
                        android:text="@string/save_contact_to_avoid"
                        android:textColor="@color/black"
                        android:textSize="@dimen/thirteen_sp"
                        android:fontFamily="@font/poppins_bold"
                        android:textStyle="bold">

                    </TextView>

                    <ImageView
                        android:layout_width="@dimen/twenty_five_sp"
                        android:layout_height="@dimen/twenty_five_sp"
                        android:layout_weight="0.1"
                        android:src="@drawable/arrow_right_gray">

                    </ImageView>
                </LinearLayout>

                <View
                    android:id="@+id/isViewSignalCheck"
                    android:layout_width="match_parent"
                    android:layout_height="0.1dp"
                    android:layout_below="@+id/lnrSaveContact"
                    android:layout_marginStart="@dimen/thirty"
                    android:layout_marginTop="@dimen/ten"
                    android:layout_marginEnd="@dimen/thirty"
                    android:background="@color/grey"/>


                <LinearLayout
                    android:id="@+id/lnrSignalCheck"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/isViewSignalCheck"
                    android:layout_marginStart="@dimen/thirty"
                    android:layout_marginTop="@dimen/twenty"
                    android:layout_marginBottom="@dimen/fifteen"
                    android:layout_marginEnd="@dimen/thirty"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:weightSum="1">

                    <ImageView
                        android:layout_width="@dimen/twenty_five"
                        android:layout_height="@dimen/twenty_five"
                        android:layout_weight="0.1"
                        android:src="@drawable/ic_contact">

                    </ImageView>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginStart="@dimen/thirty"
                        android:layout_weight="0.8"
                        android:text="Check network speed"
                        android:textColor="@color/black"
                        android:textSize="@dimen/thirteen_sp"
                        android:fontFamily="@font/poppins_bold"
                        android:textStyle="bold">

                    </TextView>

                    <ImageView
                        android:layout_width="@dimen/twenty_five_sp"
                        android:layout_height="@dimen/twenty_five_sp"
                        android:layout_weight="0.1"
                        android:src="@drawable/arrow_right_gray">

                    </ImageView>
                </LinearLayout>



            </RelativeLayout>


            <TextView
                android:id="@+id/lblFeedBack"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/rytGeneral"
                android:layout_marginStart="@dimen/twenty"
                android:layout_marginTop="@dimen/twenty"
                android:text="@string/lblFEEDBACK"
                android:textStyle="normal"
                android:fontFamily="@font/poppins_regular"
                android:textSize="@dimen/fifteen_sp">

            </TextView>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/lblFeedBack"
                android:layout_marginBottom="@dimen/fifteen"
                android:layout_marginHorizontal="@dimen/fifteen"
                android:background="@drawable/rect_white">

                <LinearLayout
                    android:id="@+id/lnrReportBug"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/thirty"
                    android:layout_marginTop="@dimen/twenty"
                    android:layout_marginEnd="@dimen/thirty"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:weightSum="1">

                    <ImageView
                        android:layout_width="@dimen/thirty"
                        android:layout_height="@dimen/thirty"
                        android:layout_weight="0.1"
                        android:src="@drawable/report_bug_icon"></ImageView>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginStart="@dimen/thirty"
                        android:layout_weight="0.8"
                        android:text="@string/lblReportbug"
                        android:textColor="@color/black"
                        android:textSize="@dimen/thirteen_sp"
                        android:fontFamily="@font/poppins_bold"
                        android:textStyle="bold">

                    </TextView>

                    <ImageView
                        android:layout_width="@dimen/twenty_five_sp"
                        android:layout_height="@dimen/twenty_five_sp"
                        android:layout_weight="0.1"
                        android:src="@drawable/arrow_right_gray">

                    </ImageView>
                </LinearLayout>

                <View
                    android:id="@+id/isViewReportBug"
                    android:layout_width="match_parent"
                    android:layout_height="0.1dp"
                    android:layout_below="@+id/lnrReportBug"
                    android:layout_marginStart="@dimen/thirty"
                    android:layout_marginTop="@dimen/ten"
                    android:layout_marginEnd="@dimen/thirty"
                    android:background="@color/grey"></View>

                <LinearLayout
                    android:id="@+id/lnrFeedBack"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/isViewReportBug"
                    android:layout_marginStart="@dimen/thirty"
                    android:layout_marginTop="@dimen/twenty"
                    android:layout_marginEnd="@dimen/thirty"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:weightSum="1">

                    <ImageView
                        android:layout_width="@dimen/thirty"
                        android:layout_height="@dimen/thirty"
                        android:layout_weight="0.1"
                        android:src="@drawable/send_feedback_icon"></ImageView>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginStart="@dimen/thirty"
                        android:layout_weight="0.8"
                        android:text="@string/lblRateus"
                        android:textColor="@color/black"
                        android:textSize="@dimen/thirteen_sp"
                        android:fontFamily="@font/poppins_bold"
                        android:textStyle="bold">

                    </TextView>

                    <ImageView
                        android:layout_width="@dimen/twenty_five_sp"
                        android:layout_height="@dimen/twenty_five_sp"
                        android:layout_weight="0.1"
                        android:src="@drawable/arrow_right_gray">

                    </ImageView>
                </LinearLayout>

                <View
                    android:id="@+id/isViewFeedBack"
                    android:layout_width="match_parent"
                    android:layout_height="0.1dp"
                    android:layout_below="@+id/lnrFeedBack"
                    android:layout_marginStart="@dimen/thirty"
                    android:layout_marginTop="@dimen/ten"
                    android:layout_marginEnd="@dimen/thirty"
                    android:background="@color/grey"></View>


                <LinearLayout
                    android:id="@+id/lnrLogout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/isViewFeedBack"
                    android:layout_marginStart="@dimen/thirty"
                    android:layout_marginTop="@dimen/twenty"
                    android:layout_marginEnd="@dimen/thirty"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:weightSum="1">

                    <ImageView
                        android:layout_width="@dimen/thirty"
                        android:layout_height="@dimen/thirty"
                        android:layout_weight="0.1"
                        android:src="@drawable/logout"></ImageView>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginStart="@dimen/thirty"
                        android:layout_weight="0.8"
                        android:text="@string/lblLogout"
                        android:textColor="@color/red"
                        android:textSize="@dimen/thirteen_sp"
                        android:fontFamily="@font/poppins_bold"
                        android:textStyle="bold">

                    </TextView>

                    <ImageView
                        android:layout_width="@dimen/twenty_five_sp"
                        android:layout_height="@dimen/twenty_five_sp"
                        android:layout_weight="0.1"
                        android:src="@drawable/arrow_right_gray">

                    </ImageView>
                </LinearLayout>

                <View
                    android:id="@+id/isViewLogout"
                    android:layout_width="match_parent"
                    android:layout_height="0.1dp"
                    android:layout_below="@+id/lnrFeedBack"
                    android:layout_marginStart="@dimen/twenty"
                    android:layout_marginTop="@dimen/ten"
                    android:layout_marginEnd="@dimen/twenty"
                    android:background="@color/grey">
                </View>

            </RelativeLayout>


        </RelativeLayout>

    </RelativeLayout>

</androidx.core.widget.NestedScrollView>